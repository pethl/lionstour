<%= simple_form_for(@scoreboard) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :fixture_id %>
    <%= f.input :user_id %>
    <%= f.input :matchscore %>
    <%= f.input :comments %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
